{
  "generalConfiguration.sortOnBeforeSave": true,
  "importStringConfiguration.maximumNumberOfImportExpressionsPerLine.type": "maxLineLength",
  "importStringConfiguration.maximumNumberOfImportExpressionsPerLine.count": 80,
  "importStringConfiguration.tabSize": 2,
  "importStringConfiguration.quoteMark": "double",
  "importSorter.sortConfiguration.customOrderingRules.rules": [
    {
      "type": "importMember",
      "regex": "^$",
      "orderLevel": 5,
      "disableSort": true
    },
    {
      "regex": "^[^.@]",
      "orderLevel": 5
    },
    {
      "regex": "^[@]",
      "orderLevel": 5
    },
    {
      "regex": "^(../){3,}",
      "orderLevel": 20
    },
    {
      "regex": "^(../){2}",
      "orderLevel": 30
    },
    {
      "regex": "^(../){1}",
      "orderLevel": 40
    },
    {
      "regex": "^[.]",
      "orderLevel": 50
    }
  ]
}
